func main(): int {
    var a: int := 3;
    var b: int := 7;

    var eq: bool := a == b;
    var neq: bool := a != b;
    var lt: bool := a < b;
    var gt: bool := a > b;
    var le: bool := a <= b;
    var ge: bool := a >= b;

    # Remember: we print all as ints (0 or 1)
    print(eq);   # 0
    print(neq);  # 1
    print(lt);   # 1
    print(gt);   # 0
    print(le);   # 1
    print(ge);   # 0

    return 0;
}
