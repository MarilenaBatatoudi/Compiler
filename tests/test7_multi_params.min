func add(a: int, b: int): int {
    return a + b;
}

func mix(a: int, b: int, c: int): int {
    var s: int := add(a, b);
    return s * c;
}

func main(): int {
    var res1: int := add(2, 3);         # 5
    var res2: int := mix(1, 2, 3);      # (1+2)*3 = 9
    print(res1);
    print(res2);
    return 0;
}
